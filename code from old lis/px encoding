function getAge() {
	// GET PATIENT BDAY
	value = $('#patient_birthday').val();
	month = value.substr(0, 2);
	day = value.substr(3, 2);
	year = value.substr(6, 4);

	dateString = year + '-' + month + '-' + day;

	// GET DATE DATE COLLECTED

	var si_serum = document.getElementById("si_serum");
  	var si_oro_naso_swab = document.getElementById("si_oro_naso_swab");
	  var si_oro_swab = document.getElementById("si_oro_swab");
	  var si_naso_swab = document.getElementById("si_naso_swab");
	  var si_other = document.getElementById("si_other");
	  
	if (si_serum.checked == true){
  		date_collected = $('#si_serum_if_yes_date_collected').val();
  	}else if(si_oro_naso_swab.checked == true){
  		date_collected = $('#si_oro_naso_swab_if_yes_date_collected').val();
  	}else if(si_oro_swab.checked == true){
  		date_collected = $('#si_oro_swab_if_yes_date_collected').val();
  	}else if(si_naso_swab.checked == true){
  		date_collected = $('#si_naso_swab_if_yes_date_collected').val();
  	}else if(si_other.checked == true){
  		date_collected = $('#si_other_if_yes_date_collected').val();
  	}else{
  		date_collected = '';
	} 
	  
	
	di_month = date_collected.substr(0, 2);
	di_day = date_collected.substr(3, 2);
	di_year = date_collected.substr(6, 4);

	date_collected_str = di_year + '-' + di_month + '-' + di_day;

	var di_date = new Date(date_collected_str);
	var birthDate = new Date(dateString);
	var age = di_date.getFullYear() - birthDate.getFullYear();

	var m = di_date.getMonth() - birthDate.getMonth();
	if (m < 0 || (m === 0 && di_date.getDate() < birthDate.getDate())) {
		age--;
	}

	$('#patient_age').val(age);
}


// select the input element
var i = document.querySelector('input#patient_birthday');
// number of charatcers before the hyphen
var n = 2;
var n2 = 4;
// run this function everytime the user release a key
i.addEventListener('keyup', function (event) { 
  // text cleared of all dashes
  var t = i.value.replace(/-/g, '');
  // save the carret position
  var s = i.selectionStart;
  // beguining string with n digits
  var b = t.substr(0, n);
  var bs1 = t.substr(0, 2); 
  var bs2 = t.substr(2, 2);
  
  // end string with what's left (maxLength)
  var e = t.substr(n);
  var e2 = t.substr(n2);
  // if there are more then n digits
  if (e.length) {
    // console  .log(b + " :: " + e);
    // join then with a dash
    if (e.length == 1){i.value = [b, e].join('-');}
    if (e.length == 3){ console.log(e2); i.value = [bs1, bs2 ,e2 ].join('-'); 


}
    
    // if the caret was before the dash put it back to the saved position
    if (s <= n) i.setSelectionRange(s, s);

    
    // if the caret was next to the dash put it after
    if (s == n+1) i.setSelectionRange(s+1, s+1);
  // if there's no end string then no need for dashes, just show the beguining
  } else i.value = b;
});



    $("#patient_lastname").on('input', function(){
      // joiweuksal();
});

    $("#patient_firstname").on('input', function(){
      // joiweuksal();
});
    $("#patient_birthday").on('input', function(){
      // joiweuksal();
});
    function joiweuksal(){

        var pt_lname = $('#patient_lastname').val();
        var pt_fname = $('#patient_firstname').val();
        var pt_bday = $('#patient_birthday').val();
        var xtoken   = $('#xtoken').val();
            $.ajax({
                url   : "https://cdcdc.dlshsi.edu.ph/CIF/fesqwew2ewfewf",
                method : "POST",
                data : {data_action: 'ddswqeq',pt_fname:pt_fname, pt_lname:pt_lname, pt_bday:pt_bday, xtoken:xtoken},
                dataType: "json",
                success : function(data){
                    if(data.msg == "issda"){
                         $('#cif-alert-exist-patient').css('display','block');
                         $('#rewq').val('true');
                    }else{
                        $('#cif-alert-exist-patient').css('display','none');
                       $('#rewq').val('false');
                    }
                }
    
            });
    }

// CHECK DUPLICATE LAB NUMBER
    $("#laboratory_number").on('change', function(){
      check_labid();
});
    $("#laboratory_number").on('input', function(){
      check_labid();
});
    function check_labid(){

        var lab_number = $('#laboratory_number').val();
          var xtoken   = $('#xtoken').val();
            $.ajax({
                url   : "https://cdcdc.dlshsi.edu.ph/CIF/oiwkspasosoU",
                method : "POST",
                data : {data_action: 'rwqetwwwU',lab_number:lab_number,xtoken:xtoken},
                dataType: "json",
                success : function(data){
                  // console.log(data);
                    if(data.msg == "issda"){
                         $('#cif-alert-exist-lab-no').css('display','block');
                         $('#qwerwww').val('true');
                    }else{
                        $('#cif-alert-exist-lab-no').css('display','none');
                       $('#qwerwww').val('false');
                    }
                }
    
            });
    }

// END


function checkselected_gender(value){
    if (value == "male") {
        $('#ci_assessed_as_high_risk_na').prop("checked", true);
        $('#ci_are_you_pregnant_na').prop("checked", true);
        $('#ci_pregnant_lmp').val('N/A');
        pregnant_fields_disabled();
    }else{
        pregnant_fields_remove_disabled();
        $('#ci_assessed_as_high_risk_na').prop("checked", false);
        $('#ci_are_you_pregnant_na').prop("checked", false);
        $('#ci_pregnant_lmp').val('');
    }   
}

function onloadCheckGender(){
    var gender = $('#patient_sex').val();
    if (gender == "male") {
        $('#ci_assessed_as_high_risk_na').prop("checked", true);
        $('#ci_are_you_pregnant_na').prop("checked", true);
        $('#ci_pregnant_lmp').val('N/A');
        pregnant_fields_disabled();
    }else{
        pregnant_fields_remove_disabled();
        $('#ci_assessed_as_high_risk_na').prop("checked", false);
        $('#ci_are_you_pregnant_na').prop("checked", false);
        $('#ci_pregnant_lmp').val('');
    } 
}

function pregnant_fields_disabled(){
    $('#ci_are_you_pregnant_yes').attr('disabled','disabled');
    $('#ci_are_you_pregnant_no').attr('disabled','disabled');
    $('#ci_are_you_pregnant_na').attr('disabled','disabled');

    $('#ci_pregnant_lmp').attr('disabled','disabled');
    $('#ci_pregnant_lmp').css('background-color','#eaecf4');

    $('#ci_assessed_as_high_risk_yes').attr('disabled','disabled');
    $('#ci_assessed_as_high_risk_no').attr('disabled','disabled');
    $('#ci_assessed_as_high_risk_na').attr('disabled','disabled');
}

function pregnant_fields_remove_disabled(){
    $('#ci_are_you_pregnant_yes').removeAttr('disabled');
    $('#ci_are_you_pregnant_no').removeAttr('disabled');
    $('#ci_are_you_pregnant_na').removeAttr('disabled');

    $('#ci_pregnant_lmp').removeAttr('disabled');
    $('#ci_pregnant_lmp').css('background-color','#fffffff');

    $('#ci_assessed_as_high_risk_yes').removeAttr('disabled');
    $('#ci_assessed_as_high_risk_no').removeAttr('disabled');
    $('#ci_assessed_as_high_risk_na').removeAttr('disabled');
}   

