    <script>
        $(document).ready(function(){
            document.title = "Update Patient CIF Record";

            $('#preloader').css('display','none'); 
            $('#side-bar-list-of-patient').addClass('active'); 
            $('#sidebarToggle')[0].click();
            
            // disabled virus & pcr
            $('#si_serum_virus_isolation_result').attr('disabled', 'disabled');
            $('#si_serum_pcr_result').attr('disabled', 'disabled');
            $('#si_oro_naso_swab_virus_isolation_result').attr('disabled', 'disabled');
            $('#si_oro_naso_swab_pcr_result').attr('disabled', 'disabled');
            $('#si_other_virus_isolation_result').attr('disabled', 'disabled');
            $('#si_other_pcr_result').attr('disabled', 'disabled');

            var th_travel_within_14_days = "0";
            var eh_exposure_to_known_covid_case = "0";
            var ci_clinical_status_at_time_of_report = "0";

            var is_tod = "";
            var is_priority = "";

            var ci_cough = "0";
            var ci_sore_throat = "0";
            var ci_cold = "0";
            var ci_shortness_diff_of_breathing = "0";

            var ci_is_there_any_history_of_other_illness = "0";
            var ci_chest_xray_done = "0";
            var ci_are_you_pregnant = "3";
            var ci_cxr_result_pneumonia = "0";

            var eh_place_with_known_covid_transmission = "0";

            var eh_work_place = "0";
            var eh_social_gathering = "0";
            var eh_health_facility = "0";
            var eh_religious_gethering = "0";            
            var eh_other = "0";            

            var si_specimen_collected = "2";

            var final_classification = "1";
            var outcome_condition_on_discharge = "notset";

            var ci_assessed_as_high_risk = "3";

            if (ci_assessed_as_high_risk == 1) {
                $('#ci_assessed_as_high_risk_yes').prop("checked", true);
            }else{
                if (ci_assessed_as_high_risk == 2) {
                    $('#ci_assessed_as_high_risk_no').prop("checked", true);
                }
                if (ci_assessed_as_high_risk == 3) {
                    $('#ci_assessed_as_high_risk_na').prop("checked", true);
                }
            }


            if (th_travel_within_14_days == 1) {
                $('#th_travel_within_14_days_yes').prop("checked", true);
            }else{
                if (th_travel_within_14_days == 2) {
                    $('#th_travel_within_14_days_no').prop("checked", true);
                }
            }

            if (eh_exposure_to_known_covid_case == 1) {
                $('#eh_exposure_to_known_covid_case_yes').prop("checked", true);
            }else if (eh_exposure_to_known_covid_case == 2){
                $('#eh_exposure_to_known_covid_case_no').prop("checked", true);
            }else{
                if (eh_exposure_to_known_covid_case == 3) {
                    $('#eh_exposure_to_known_covid_case_unknown').prop("checked", true);
                }
            }

            if (eh_place_with_known_covid_transmission == 1) {
                $('#eh_place_with_known_covid_transmission_yes').prop("checked", true);
            }else if (eh_place_with_known_covid_transmission == 2){
                $('#eh_place_with_known_covid_transmission_no').prop("checked", true);
            }else{
                if (eh_place_with_known_covid_transmission == 3) {
                    $('#eh_place_with_known_covid_transmission_unknown').prop("checked", true);
                }
            }

            if (ci_clinical_status_at_time_of_report == 1) {
                $('#ci_clinical_status_at_time_of_report_inpatient').prop("checked", true);
            }else if (ci_clinical_status_at_time_of_report == 2){
                $('#ci_clinical_status_at_time_of_report_outpatient').prop("checked", true);
            }else if (ci_clinical_status_at_time_of_report == 3){
                $('#ci_clinical_status_at_time_of_report_died').prop("checked", true);
            }else if (ci_clinical_status_at_time_of_report == 4){
                $('#ci_clinical_status_at_time_of_report_discharged').prop("checked", true);
            }else{
                if (ci_clinical_status_at_time_of_report == 5) {
                    $('#ci_clinical_status_at_time_of_report_unknown').prop("checked", true);
                }
                
            }

            if (is_tod == 1) {
                $('#chk_tod_patient').prop("checked", true);
            }
            if (is_priority == 1) {
                $('#chk_prio_patient').prop("checked", true);
                $('#lblPriority').css('display','block');
            }else{
                $('#lblPriority').css('display','none');
            }

            if (ci_cough == 1) {
                $('#ci_cough').prop("checked", true);
            }
            if (ci_sore_throat == 1) {
                $('#ci_sore_throat').prop("checked", true);
            }
            if (ci_cold == 1) {
                $('#ci_cold').prop("checked", true);
            }
            if (ci_shortness_diff_of_breathing == 1) {
                $('#ci_shortness_diff_of_breathing').prop("checked", true);
            }

            var ci_as = "0";
            if (ci_as == 1) {
                $('#ci_asymptomatic').prop("checked", true);
            }

            // PLACES
            if (eh_work_place == 1) {
                $('#eh_work_place').prop("checked", true);
            }
            if (eh_social_gathering == 1) {
                $('#eh_social_gathering').prop("checked", true);
            }
            if (eh_health_facility == 1) {
                $('#eh_health_facility').prop("checked", true);
            }
            if (eh_religious_gethering == 1) {
                $('#eh_religious_gethering').prop("checked", true);
            }
            if (eh_other == 1) {
                $('#eh_other').prop("checked", true);
            }

            if (ci_is_there_any_history_of_other_illness == 1) {
                $('#ci_is_there_any_history_of_other_illness_yes').prop("checked", true);
            }else{
                if (ci_is_there_any_history_of_other_illness == 2) {
                    $('#ci_is_there_any_history_of_other_illness_no').prop("checked", true);
                }
                
            }

            if (ci_chest_xray_done == 1) {
                $('#ci_chest_xray_done_yes').prop("checked", true);
            }else{
                if (ci_chest_xray_done == 2) {
                    $('#ci_chest_xray_done_no').prop("checked", true);
                    no_xray_done();
                }
                
            }

            if (ci_are_you_pregnant == 1) {
                $('#ci_are_you_pregnant_yes').prop("checked", true);
            }else{
                if (ci_are_you_pregnant == 2) {
                    $('#ci_are_you_pregnant_no').prop("checked", true);
                }
                if (ci_are_you_pregnant == 3) {
                    $('#ci_are_you_pregnant_na').prop("checked", true);
                }
                
            }

            if (ci_cxr_result_pneumonia == 1) {
                $('#ci_cxr_result_pneumonia_yes').prop("checked", true);
            }else if (ci_cxr_result_pneumonia == 2){
                $('#ci_cxr_result_pneumonia_no').prop("checked", true);
            }else{
                if (ci_cxr_result_pneumonia == 3) {
                    $('#ci_cxr_result_pneumonia_pending').prop("checked", true);
                }
                if (ci_cxr_result_pneumonia == 4) {
                    $('#ci_cxr_result_pneumonia_na').prop("checked", true);
                }
                
            }
            var has_specimen = false;
            if (si_specimen_collected == 1) {
                $('#si_serum').prop("checked", true);
                has_specimen = true;
                check_specimen_serum();
                $('#si_serum_if_yes_date_collected').val("12-21-2020");
                $('#si_serum_date_sent_to_ritm').val("");
                $('#si_serum_date_received_in_ritm').val("12-21-2020");

                // SET TIME
                $('#si_serum_if_yes_date_collected_hour').val("7");
                $('#si_serum_if_yes_date_collected_minutes').val("45");
                $('#si_serum_date_sent_to_ritm_hour').val("");
                $('#si_serum_date_sent_to_ritm_minutes').val("");
                $('#si_serum_date_received_in_ritm_hour').val("");
                $('#si_serum_date_received_in_ritm_minutes').val("");
            }
            if (si_specimen_collected == 2) {
                $('#si_oro_naso_swab').prop("checked", true);
                has_specimen = true;
                check_specimen_oro();
                $('#si_oro_naso_swab_if_yes_date_collected').val("12-21-2020");
                $('#si_oro_naso_swab_date_sent_to_ritm').val("");
                $('#si_oro_naso_swab_date_received_in_ritm').val("12-21-2020");

                // SET TIME
                $('#si_oro_naso_swab_if_yes_date_collected_hour').val("7");
                $('#si_oro_naso_swab_if_yes_date_collected_minutes').val("45");
                $('#si_oro_naso_swab_date_sent_to_ritm_hour').val("");
                $('#si_oro_naso_swab_date_sent_to_ritm_minutes').val("");
                $('#si_oro_naso_swab_date_received_in_ritm_hour').val("");
                $('#si_oro_naso_swab_date_received_in_ritm_minutes').val("");
            }
            if (si_specimen_collected == 3) {
                $('#si_other').prop("checked", true);
                has_specimen = true;
                check_specimen_other();
                $('#si_other_if_yes_date_collected').val("12-21-2020");
                $('#si_other_date_sent_to_ritm').val("");
                $('#si_other_date_received_in_ritm').val("12-21-2020");

                // SET TIME
                $('#si_other_if_yes_date_collected_hour').val("7");
                $('#si_other_if_yes_date_collected_minutes').val("45");
                $('#si_other_date_sent_to_ritm_hour').val("");
                $('#si_other_date_sent_to_ritm_minutes').val("");
                $('#si_other_date_received_in_ritm_hour').val("");
                $('#si_other_date_received_in_ritm_minutes').val("");
            }
            // ORO
            if (si_specimen_collected == 4) {
                $('#si_oro_swab').prop("checked", true);
                has_specimen = true;
                check_specimen_oro_sp();
                $('#si_oro_swab_if_yes_date_collected').val("12-21-2020");
                $('#si_oro_swab_date_sent_to_ritm').val("");
                $('#si_oro_swab_date_received_in_ritm').val("12-21-2020");

                // SET TIME
                $('#si_oro_swab_if_yes_date_collected_hour').val("7");
                $('#si_oro_swab_if_yes_date_collected_minutes').val("45");
                $('#si_oro_swab_date_sent_to_ritm_hour').val("");
                $('#si_oro_swab_date_sent_to_ritm_minutes').val("");
                $('#si_oro_swab_date_received_in_ritm_hour').val("");
                $('#si_oro_swab_date_received_in_ritm_minutes').val("");
            }
            // NASO
            if (si_specimen_collected == 5) {
                $('#si_naso_swab').prop("checked", true);
                has_specimen = true;
                check_specimen_naso();
                $('#si_naso_swab_if_yes_date_collected').val("12-21-2020");
                $('#si_naso_swab_date_sent_to_ritm').val("");
                $('#si_naso_swab_date_received_in_ritm').val("12-21-2020");

                // SET TIME
                $('#si_naso_swab_if_yes_date_collected_hour').val("7");
                $('#si_naso_swab_if_yes_date_collected_minutes').val("45");
                $('#si_naso_swab_date_sent_to_ritm_hour').val("");
                $('#si_naso_swab_date_sent_to_ritm_minutes').val("");
                $('#si_naso_swab_date_received_in_ritm_hour').val("");
                $('#si_naso_swab_date_received_in_ritm_minutes').val("");
            }

            if (has_specimen == false) {
                specimen_disbled();
            }

            if (final_classification == 1) {
                $('#final_classification_pui').prop("checked", true);
            }else if (final_classification == 2){
                $('#final_classification_pum').prop("checked", true);
            }else{
                if (final_classification == 3) {
                    $('#final_classification_confirmed').prop("checked", true);
                }
            }

            if (outcome_condition_on_discharge == 1) {
                $('#outcome_condition_on_discharge_died').prop("checked", true);
            }else if (outcome_condition_on_discharge == 2){
                $('#outcome_condition_on_discharge_improved').prop("checked", true);
            }else if (outcome_condition_on_discharge == 3){
                $('#outcome_condition_on_discharge_recovered').prop("checked", true);
            }else if (outcome_condition_on_discharge == 4){
                $('#outcome_condition_on_discharge_transferred').prop("checked", true);
            }else{
                if (outcome_condition_on_discharge == 5) {
                    $('#outcome_condition_on_discharge_absconded').prop("checked", true);
                }
                
            }
            

            $('#patient_sex').val("male");
            onloadCheckGender();
            if ( $('#patient_sex').val() == "male") {
                pregnant_fields_disabled();
            }


            $('#patient_civil_status').val("single");

            


            loaddatepicker();

            // Permanent address load
            $('#ph_residence_region').val("NCR - National Capital Region");
            getProvince("NCR - National Capital Region");
            $('#ph_residence_province').val("City Of Manila, Fourth District");
            getMunicipalities("City Of Manila, Fourth District");
            $('#ph_residence_city_municipality').val("Muntinlupa City");
            

            // Current address load
            $('#ph_residence_region_current').val("");
            getProvince_current("");
            $('#ph_residence_province_current').val("");
            getMunicipalities_current("");
            $('#ph_residence_city_municipality_current').val("");
            

            // LOAD PERSONS
            var persons_array = [];
            var contact_array = [];

            for (var i = 0; i < persons_array.length; i++) {
                $('#TP_'+i).val(persons_array[i]);
                $('#TC_'+i).val(contact_array[i]);
            }
            if( persons_array.length >= 9){
                $('#additional_names').css('display','flex');
                $('#btnAdditionalNames').css('display','none');
            }
});
// DOCUMENT ONLOAD END

function add_more_names(){
    $('#additional_names').css('display','flex');
    $('#btnAdditionalNames').css('display','none');
}

function same_as_permanent(){
    $('#ph_residence_house_no_lot_bldg_current').val($('#ph_residence_house_no_lot_bldg').val());
    $('#ph_residence_street_current').val($('#ph_residence_street').val());
    
    $('#ph_residence_home_phone_no_current').val($('#ph_residence_home_phone_no').val());
    $('#ph_residence_cellphone_no_current').val($('#ph_residence_cellphone_no').val());
    $('#ph_residence_email_add_current').val($('#ph_residence_email_add').val());

    $('#ph_residence_region_current').val($('#ph_residence_region').val()); 
    getProvince_current($('#ph_residence_region').val());
    $('#ph_residence_province_current').val($('#ph_residence_province').val());
    
    getMunicipalities_current($('#ph_residence_province').val());
    $('#ph_residence_city_municipality_current').val($('#ph_residence_city_municipality').val());
    

    
    disable_current();
}

function disable_current(){
    $('#ph_residence_house_no_lot_bldg_current').attr('disabled','disabled');
    $('#ph_residence_street_current').attr('disabled','disabled');
    $('#ph_residence_province_current').attr('disabled','disabled');
    $('#ph_residence_city_municipality_current').attr('disabled','disabled');
    $('#ph_residence_region_current').attr('disabled','disabled');
    $('#ph_residence_home_phone_no_current').attr('disabled','disabled');
    $('#ph_residence_cellphone_no_current').attr('disabled','disabled');
    $('#ph_residence_email_add_current').attr('disabled','disabled');
}

function remove_disable_current(){
    $('#ph_residence_house_no_lot_bldg_current').removeAttr('disabled');
    $('#ph_residence_street_current').removeAttr('disabled');
    $('#ph_residence_province_current').removeAttr('disabled');
    $('#ph_residence_city_municipality_current').removeAttr('disabled');
    $('#ph_residence_region_current').removeAttr('disabled');
    $('#ph_residence_home_phone_no_current').removeAttr('disabled');
    $('#ph_residence_cellphone_no_current').removeAttr('disabled');
    $('#ph_residence_email_add_current').removeAttr('disabled');
}

