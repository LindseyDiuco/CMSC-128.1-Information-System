	    $('#si_naso_swab_date_sent_to_ritm').attr('disabled', 'disabled');
	    $('#si_naso_swab_date_received_in_ritm').attr('disabled', 'disabled');

	    // RESET TIME VALUE
	    $('#si_naso_swab_if_yes_date_collected_hour').val('');
	    $('#si_naso_swab_if_yes_date_collected_minutes').val('');
	   
	    $('#si_naso_swab_date_sent_to_ritm_hour').val('');
	    $('#si_naso_swab_date_sent_to_ritm_minutes').val('');
	 
	    $('#si_naso_swab_date_received_in_ritm_hour').val('');
	    $('#si_naso_swab_date_received_in_ritm_minutes').val('');

	    // DISABLED TIME
	    $('#si_naso_swab_if_yes_date_collected_hour').attr('disabled', 'disabled');
	    $('#si_naso_swab_if_yes_date_collected_minutes').attr('disabled', 'disabled');

	    $('#si_naso_swab_date_sent_to_ritm_hour').attr('disabled', 'disabled');
	    $('#si_naso_swab_date_sent_to_ritm_minutes').attr('disabled', 'disabled');

	    $('#si_naso_swab_date_received_in_ritm_hour').attr('disabled', 'disabled');
	    $('#si_naso_swab_date_received_in_ritm_minutes').attr('disabled', 'disabled');
	    
	// END

    //5th ROW

	    $('#si_other_if_yes_date_collected').css('background-color', '#ffffff');
	    $('#si_other_date_sent_to_ritm').css('background-color', '#ffffff');
	    $('#si_other_date_received_in_ritm').css('background-color', '#ffffff');

	    $('#si_other_if_yes_date_collected').removeAttr('disabled');
	    $('#si_other_date_sent_to_ritm').removeAttr('disabled');
	    $('#si_other_date_received_in_ritm').removeAttr('disabled');

	    // DISABLED TIME
	    $('#si_other_if_yes_date_collected_hour').removeAttr('disabled');
    	$('#si_other_if_yes_date_collected_minutes').removeAttr('disabled');

	    $('#si_other_date_sent_to_ritm_hour').removeAttr('disabled');
    	$('#si_other_date_sent_to_ritm_minutes').removeAttr('disabled');

	    $('#si_other_date_received_in_ritm_hour').removeAttr('disabled');
    	$('#si_other_date_received_in_ritm_minutes').removeAttr('disabled');

    	// AST
	    $('#ast_date_serum').css('display','none');

		$('#ast_date_oro').css('display','none');
		$('#ast_date_oro_sp').css('display','none');
		$('#ast_date_naso').css('display','none');
		$('#ast_date_other').css('display','inline-block');	 
    
    $('#ast_date_received_serum').css('display','none');

		$('#ast_date_received_oro').css('display','none');
		$('#ast_date_received_oro_sp').css('display','none');
		$('#ast_date_received_naso').css('display','none');
		$('#ast_date_received_other').css('display','inline-block');	 
    // END
}

var myRadios = document.getElementsByName('th_travel_within_14_days');
var setCheck;
var x = 0;
for(x = 0; x < myRadios.length; x++){

    myRadios[x].onclick = function(){
        if(setCheck != this){
             setCheck = this;
        }else{
            this.checked = false;
            setCheck = null;
    }
    };

}

