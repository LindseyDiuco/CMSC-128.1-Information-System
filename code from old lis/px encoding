$('#si_other_date_sent_to_ritm').change(function() { 
    var date_collected = new Date($('#si_other_if_yes_date_collected').val());
    var date_send = new Date($('#si_other_date_sent_to_ritm').val());
    var date_received = new Date($('#si_other_date_received_in_ritm').val());

    if (date_send < date_collected || date_send > date_received) {
        $('#si_other_date_sent_to_ritm').val('');
        $('#specimenDatesModal').modal('toggle');
    }
});

$('#si_other_date_received_in_ritm').change(function() { 
    var date_collected = new Date($('#si_other_if_yes_date_collected').val());
    var date_send = new Date($('#si_other_date_sent_to_ritm').val());
    var date_received = new Date($('#si_other_date_received_in_ritm').val());

    if (date_received < date_send || date_received < date_collected) {
        $('#si_other_date_received_in_ritm').val('');
        $('#specimenDatesModal').modal('toggle');
    }
});


