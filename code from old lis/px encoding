function no_xray_done(){
  if(document.getElementById('ci_chest_xray_done_no').checked) {
      $('#ci_cxr_result_pneumonia_na').prop("checked",true);
      $('#ci_other_radiologic_findings').val("N/A");
      no_xray_done_disabled();
  }else{
    no_xray_done_remove_disabled();
      $('#ci_cxr_result_pneumonia_na').prop("checked",false);
      $('#ci_other_radiologic_findings').val('');

  }
}

function no_xray_done_disabled(){
  $('#ci_chest_xray_yes_specify').attr('disabled','disabled');
  $('#ci_cxr_result_pneumonia_yes').attr('disabled','disabled');
  $('#ci_cxr_result_pneumonia_no').attr('disabled','disabled');
  $('#ci_cxr_result_pneumonia_pending').attr('disabled','disabled');
  $('#ci_cxr_result_pneumonia_na').attr('disabled','disabled');
  $('#ci_other_radiologic_findings').attr('disabled','disabled');
}

function no_xray_done_remove_disabled(){
  $('#ci_chest_xray_yes_specify').removeAttr('disabled');
  $('#ci_cxr_result_pneumonia_yes').removeAttr('disabled');
  $('#ci_cxr_result_pneumonia_no').removeAttr('disabled');
  $('#ci_cxr_result_pneumonia_pending').removeAttr('disabled');
  $('#ci_cxr_result_pneumonia_na').removeAttr('disabled');
  $('#ci_other_radiologic_findings').removeAttr('disabled');
}



function check_specimen_serum() {
  // Get the checkbox
  var si_serum = document.getElementById("si_serum");
  // Get the output text
  

  // If the checkbox is checked, display the output text
  if (si_serum.checked == true){
    check_serum();
    return;
  } else {
    specimen_disbled();
    return;
  }

}

