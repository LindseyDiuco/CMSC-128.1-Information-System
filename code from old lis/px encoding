	$('#si_other_date_received_in_ritm_minutes').attr('disabled', 'disabled');

	$('#si_other_virus_isolation_result').attr('disabled', 'disabled');
	$('#si_other_pcr_result').attr('disabled', 'disabled');
    // END


    // AST
	$('#ast_date_serum').css('display','none');
	$('#ast_date_oro').css('display','none');
	$('#ast_date_oro_sp').css('display','none');
	$('#ast_date_naso').css('display','none');
	$('#ast_date_other').css('display','none');	


  $('#ast_date_received_serum').css('display','none');
	$('#ast_date_received_oro').css('display','none');
	$('#ast_date_received_oro_sp').css('display','none');
	$('#ast_date_received_naso').css('display','none');
	$('#ast_date_received_other').css('display','none');	
}
function submit_validate_form(){
  var laboratory_number = $('#laboratory_number').val();
    var patient_lastname = $('#patient_lastname').val();
    var patient_firstname = $('#patient_firstname').val();
    var patient_birthday = $('#patient_birthday').val();
    var patient_age = $('#patient_age').val();
    var patient_sex = $('#patient_sex').val();
    var patient_civil_status = $('#patient_civil_status').val();
    var disease_reporting_unit_hospital = $('#disease_reporting_unit_hospital').val();
    var name_of_investigator = $('#name_of_investigator').val();
    var date_of_interview = $('#date_of_interview').val();
    var patient_middle_name = $('#patient_middle_name').val();

    var ofw_employers_name = $('#ofw_employers_name').val();

    // PH Residence
    var ph_residence_city_municipality = $('#ph_residence_city_municipality').val().trim();
    var ph_residence_province = $('#ph_residence_province').val().trim();
    var ph_residence_region = $('#ph_residence_region').val().trim();

    var specimen_information = check_sp();

  if(specimen_information == false || ofw_employers_name == '' || patient_middle_name == '' || laboratory_number == '' || patient_lastname == '' || patient_firstname == '' || patient_birthday  == '' || patient_age  == '' || patient_sex  == '' ||  disease_reporting_unit_hospital == '' || date_of_interview == '' || ph_residence_city_municipality == '' || ph_residence_province == '' || ph_residence_region == ''){
     
    $('#cif-alert-create-success').css('display','none');
    $('#cif-alert-empty-fields').css('display','block');
     $('#has_changes').val('1');
  }else{


    var rewq = $('#rewq').val();
        var qwerwww = $('#qwerwww').val();
            if (rewq == "false" && qwerwww == "false") {
              $('#cif-alert-empty-fields').css('display','none'); 
              if($('#has_changes').val() == '0'){
                $('#ValidateModal').modal('toggle');
              }else{
                checkFields_update_with_remarks();
              }
              
              
            }
    
  }
    
}

function check_tod_patient(){
  var chk_tod_patient = document.getElementById("chk_tod_patient");
  if (chk_tod_patient.checked == true){
    var lab_number = $('#laboratory_number').val() + '-TOD';
      $('#laboratory_number').val(lab_number);
    }else{
      var lab_number = $('#laboratory_number').val();
      lab_number = lab_number.replace('-TOD','');
      $('#laboratory_number').val(lab_number);
    } 
    check_labid();
}

function check_priority(){
    var chk_prio_patient = document.getElementById("chk_prio_patient");
      if (chk_prio_patient.checked == true){
             $('#lblPriority').css('display','block');
        }else{
          $('#lblPriority').css('display','none');
        } 
   
}

function submit_validate_form_proceed(){
  $('#etadilav_form').submit();
}

function submit_validate_withremarks_form(){
  removedisabled();
    var modal_remarks = $('#qc_validation_remarks').val();
    var is_validated = "1";
    $('#v_remarks').val(modal_remarks);
    // alert(modal_remarks);
    $('#is_make_validation').val('vup');
    $('#is_validated').val(is_validated);
    $('#update_form').submit();
}

function isNumberKey_with_dash(evt){
  
  var charCode = (evt.which) ? evt.which : event.keyCode
    
    if (charCode > 31 && (charCode < 48 || charCode > 57)){
      if (charCode == 45) {
          return true;
      }else{
          return false;
      }
    }else{
        return true;  
    }            

}

function isNumberKey(evt)
      {
         var charCode = (evt.which) ? evt.which : event.keyCode
         if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;

         return true;
      }    

