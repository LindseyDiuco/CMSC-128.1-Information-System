// GER PROVINCE
function getProvince(region){
    
    if (region == "" || region != "REGION IV-A" || region != "NCR" || region != "REGION-III" || region != "REGION-V" || region != "ILOCOS REGION") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').append($("<option>").attr('value','').text('- - -'));
    }

    if (region == "ILOCOS REGION") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').removeAttr('disabled');
      load_ilocos_region_provinces();
      $('#ph_residence_city_municipality').empty();
      
    }

    if (region == "REGION-III") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').removeAttr('disabled');
      load_region_3_provinces();
      $('#ph_residence_city_municipality').empty();
      
    }
    if (region == "REGION-V") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').removeAttr('disabled');
      load_region_5_provinces();
      $('#ph_residence_city_municipality').empty();
    }

    if (region == "REGION IV-A") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').removeAttr('disabled');
      load_region_4a_provinces();
      $('#ph_residence_city_municipality').empty();
    }
    if (region == "NCR") {
      $('#ph_residence_province').empty();
      $('#ph_residence_province').removeAttr('disabled');
      load_ncr_provinces();
      $('#ph_residence_city_municipality').empty();
      load_ncr_municipalities();
    }
    
}

function getProvince_current(region){
    
    if (region == "" || region != "REGION IV-A" || region != "NCR" || region != "REGION-III" || region != "REGION-V" || region != "ILOCOS REGION") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').append($("<option>").attr('value','').text('- - -'));
      $('#ph_residence_city_municipality_current').empty();
      $('#ph_residence_city_municipality_current').append($("<option>").attr('value','').text('- - -'));
    }

    if (region == "ILOCOS REGION") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').removeAttr('disabled');
      load_ilocos_region_provinces_current();
      $('#ph_residence_city_municipality_current').empty();
      
    }
    if (region == "REGION-III") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').removeAttr('disabled');
      load_region_3_provinces_current();
      $('#ph_residence_city_municipality_current').empty();
      
    }
    if (region == "REGION-V") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').removeAttr('disabled');
      load_region_5_provinces_current();
      $('#ph_residence_city_municipality_current').empty();
    }
    
    if (region == "REGION IV-A") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').removeAttr('disabled');
      load_region_4a_provinces_current();
      $('#ph_residence_city_municipality_current').empty();
    }
    if (region == "NCR") {
      $('#ph_residence_province_current').empty();
      $('#ph_residence_province_current').removeAttr('disabled');
      load_ncr_provinces_current();
      $('#ph_residence_city_municipality_current').removeAttr('disabled');
      $('#ph_residence_city_municipality_current').empty();
      load_ncr_municipalities_current();
    }
    
}
// END

// LOAD PROVINCES PERMANENT
function load_ilocos_region_provinces(){

  var arr = [
    {val : '', text: '- - -'},
    {val : 'ILOCOS NORTE', text: 'ILOCOS NORTE'},
    {val : 'ILOCOS SUR', text: 'ILOCOS SUR'},
    {val : 'LA UNION', text: 'LA UNION'},
    {val : 'PANGASINAN', text: 'PANGASINAN'}
  ];

  $(arr).each(function() {
    $('#ph_residence_province').append($("<option>").attr('value',this.val).text(this.text));
  });

}
function load_region_3_provinces(){

  var arr = [
    {val : '', text: '- - -'},
    {val : 'AURORA', text: 'AURORA'},
    {val : 'BATAAN', text: 'BATAAN'},
    {val : 'BULACAN', text: 'BULACAN'},
    {val : 'NUEVA ECIJA', text: 'NUEVA VIZCAYA'},
    {val : 'PAMPANGA', text: 'PAMPANGA'},
    {val : 'TARLAC', text: 'TARLAC'},
    {val : 'ZAMBALES', text: 'ZAMBALES'}
  ];

  $(arr).each(function() {
    $('#ph_residence_province').append($("<option>").attr('value',this.val).text(this.text));
  });

}
function load_region_4a_provinces(){

    var arr = [
      {val : '', text: '- - -'},
      {val : 'BATANGAS', text: 'BATANGAS'},
      {val : 'CAVITE', text: 'CAVITE'},
      {val : 'LAGUNA', text: 'LAGUNA'},
      {val : 'RIZAL', text: 'RIZAL'}
    ];

    $(arr).each(function() {
        $('#ph_residence_province').append($("<option>").attr('value',this.val).text(this.text));
    });

}
