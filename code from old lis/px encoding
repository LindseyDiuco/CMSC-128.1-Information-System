		$('#si_serum_if_yes_date_collected').val('');
	    $('#si_serum_date_sent_to_ritm').val('');
	    $('#si_serum_date_received_in_ritm').val('');

		$('#si_serum_if_yes_date_collected').css('background-color', '#eaecf4');
	    $('#si_serum_date_sent_to_ritm').css('background-color', '#eaecf4');
	    $('#si_serum_date_received_in_ritm').css('background-color', '#eaecf4');

		$('#si_serum_if_yes_date_collected').attr('disabled', 'disabled');
	    $('#si_serum_date_sent_to_ritm').attr('disabled', 'disabled');
	    $('#si_serum_date_received_in_ritm').attr('disabled', 'disabled');

	    // RESET TIME VALUE
	    $('#si_serum_if_yes_date_collected_hour').val('');
	    $('#si_serum_if_yes_date_collected_minutes').val('');

	    $('#si_serum_date_sent_to_ritm_hour').val('');
	    $('#si_serum_date_sent_to_ritm_minutes').val('');
	    
	    $('#si_serum_date_received_in_ritm_hour').val('');
	    $('#si_serum_date_received_in_ritm_minutes').val('');


	    // DISABLED TIME
	    $('#si_serum_if_yes_date_collected_hour').attr('disabled', 'disabled');
	    $('#si_serum_if_yes_date_collected_minutes').attr('disabled', 'disabled');

	    $('#si_serum_date_sent_to_ritm_hour').attr('disabled', 'disabled');
	    $('#si_serum_date_sent_to_ritm_minutes').attr('disabled', 'disabled');
	    
	    $('#si_serum_date_received_in_ritm_hour').attr('disabled', 'disabled');
	    $('#si_serum_date_received_in_ritm_minutes').attr('disabled', 'disabled');

	// END

  	//2ND ROW
	  	$('#si_oro_naso_swab').prop("checked", false);

	  	$('#si_oro_naso_swab_if_yes_date_collected').val('');
	    $('#si_oro_naso_swab_date_sent_to_ritm').val('');
	    $('#si_oro_naso_swab_date_received_in_ritm').val('');

	    $('#si_oro_naso_swab_if_yes_date_collected').css('background-color', '#eaecf4');
	    $('#si_oro_naso_swab_date_sent_to_ritm').css('background-color', '#eaecf4');
	    $('#si_oro_naso_swab_date_received_in_ritm').css('background-color', '#eaecf4');

	    $('#si_oro_naso_swab_if_yes_date_collected').attr('disabled', 'disabled');
	    $('#si_oro_naso_swab_date_sent_to_ritm').attr('disabled', 'disabled');
	    $('#si_oro_naso_swab_date_received_in_ritm').attr('disabled', 'disabled');

	    // RESET TIME VALUE
	    $('#si_oro_naso_swab_if_yes_date_collected_hour').val('');
	    $('#si_oro_naso_swab_if_yes_date_collected_minutes').val('');
	   
	    $('#si_oro_naso_swab_date_sent_to_ritm_hour').val('');
	    $('#si_oro_naso_swab_date_sent_to_ritm_minutes').val('');
	 
	    $('#si_oro_naso_swab_date_received_in_ritm_hour').val('');
	    $('#si_oro_naso_swab_date_received_in_ritm_minutes').val('');
	    // END

	    // DISABLED TIME
	    $('#si_oro_naso_swab_if_yes_date_collected_hour').attr('disabled', 'disabled');
	    $('#si_oro_naso_swab_if_yes_date_collected_minutes').attr('disabled', 'disabled');

	    $('#si_oro_naso_swab_date_sent_to_ritm_hour').attr('disabled', 'disabled');
	    $('#si_oro_naso_swab_date_sent_to_ritm_minutes').attr('disabled', 'disabled');

	    $('#si_oro_naso_swab_date_received_in_ritm_hour').attr('disabled', 'disabled');
	    $('#si_oro_naso_swab_date_received_in_ritm_minutes').attr('disabled', 'disabled');

    // END

	//3rd ROW
	    $('#si_oro_swab').prop("checked", false);

	    $('#si_oro_swab_if_yes_date_collected').val('');
	    $('#si_oro_swab_date_sent_to_ritm').val('');
	    $('#si_oro_swab_date_received_in_ritm').val('');

	    $('#si_oro_swab_if_yes_date_collected').css('background-color', '#eaecf4');
	    $('#si_oro_swab_date_sent_to_ritm').css('background-color', '#eaecf4');
	    $('#si_oro_swab_date_received_in_ritm').css('background-color', '#eaecf4');

	    $('#si_oro_swab_if_yes_date_collected').attr('disabled', 'disabled');
	    $('#si_oro_swab_date_sent_to_ritm').attr('disabled', 'disabled');
	    $('#si_oro_swab_date_received_in_ritm').attr('disabled', 'disabled');

	    // RESET TIME VALUE
	    $('#si_oro_swab_if_yes_date_collected_hour').val('');
	    $('#si_oro_swab_if_yes_date_collected_minutes').val('');
	   
	    $('#si_oro_swab_date_sent_to_ritm_hour').val('');
	    $('#si_oro_swab_date_sent_to_ritm_minutes').val('');
	 
	    $('#si_oro_swab_date_received_in_ritm_hour').val('');
	    $('#si_oro_swab_date_received_in_ritm_minutes').val('');

	    // DISABLED TIME
	    $('#si_oro_swab_if_yes_date_collected_hour').attr('disabled', 'disabled');
	    $('#si_oro_swab_if_yes_date_collected_minutes').attr('disabled', 'disabled');

	    $('#si_oro_swab_date_sent_to_ritm_hour').attr('disabled', 'disabled');
	    $('#si_oro_swab_date_sent_to_ritm_minutes').attr('disabled', 'disabled');

	    $('#si_oro_swab_date_received_in_ritm_hour').attr('disabled', 'disabled');
	    $('#si_oro_swab_date_received_in_ritm_minutes').attr('disabled', 'disabled');
	    
	// END

	//4th ROW
	    $('#si_naso_swab').prop("checked", false);

	    $('#si_naso_swab_if_yes_date_collected').val('');
	    $('#si_naso_swab_date_sent_to_ritm').val('');
	    $('#si_naso_swab_date_received_in_ritm').val('');

