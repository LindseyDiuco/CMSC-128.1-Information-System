// NOT ALLOWED NUMBERS
$('#patient_lastname').keydown(function (e) {
    
    if (e.shiftKey || e.altKey) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        e.preventDefault();
      }
    }
 });
$('#patient_firstname').keydown(function (e) {
    if (e.shiftKey || e.altKey ) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        e.preventDefault();
      }
    }
 });
$('#patient_middle_name').keydown(function (e) {
    if (e.shiftKey || e.altKey) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        if(key != 189){
            e.preventDefault();
        }
      }
    }
 });
$('#name_of_investigator').keydown(function (e) {
    if (e.shiftKey || e.altKey) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        e.preventDefault();
      }
    }
 });
$('#other_investigator').keydown(function (e) {
    if (e.shiftKey || e.altKey) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 188) || (key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90) )) {
        console.log(key);
        e.preventDefault();
      }
    }
 });
$('#outcome_name_of_informant').keydown(function (e) {
    if (e.shiftKey || e.altKey) {
      e.preventDefault();
    }else{
      var key = e.keyCode;
      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        e.preventDefault();
      }
    }
 });


function removedisabled(){
    //1ST ROW  
        $('#si_serum_if_yes_date_collected').removeAttr('disabled');
        $('#si_serum_date_sent_to_ritm').removeAttr('disabled');
        $('#si_serum_date_received_in_ritm').removeAttr('disabled');
        $('#si_other_virus_isolation_result').removeAttr('disabled');
        $('#si_other_pcr_result_specify').removeAttr('disabled');
        $('#si_serum_virus_isolation_result').removeAttr('disabled');
        $('#si_serum_pcr_result').removeAttr('disabled');

        // TIME
        $('#si_serum_if_yes_date_collected_hour').removeAttr('disabled');
        $('#si_serum_if_yes_date_collected_minutes').removeAttr('disabled');

        $('#si_serum_date_sent_to_ritm_hour').removeAttr('disabled');
        $('#si_serum_date_sent_to_ritm_minutes').removeAttr('disabled');

        $('#si_serum_date_received_in_ritm_hour').removeAttr('disabled');
        $('#si_serum_date_received_in_ritm_minutes').removeAttr('disabled');

    // END

    //2ND ROW
        $('#si_oro_naso_swab_if_yes_date_collected').removeAttr('disabled');
        $('#si_oro_naso_swab_date_sent_to_ritm').removeAttr('disabled');
        $('#si_oro_naso_swab_date_received_in_ritm').removeAttr('disabled');
        $('#si_oro_naso_swab_virus_isolation_result').removeAttr('disabled');
        $('#si_oro_naso_swab_pcr_result').removeAttr('disabled');

        // TIME
        $('#si_oro_naso_swab_if_yes_date_collected_hour').removeAttr('disabled');
        $('#si_oro_naso_swab_if_yes_date_collected_minutes').removeAttr('disabled');

        $('#si_oro_naso_swab_date_sent_to_ritm_hour').removeAttr('disabled');
        $('#si_oro_naso_swab_date_sent_to_ritm_minutes').removeAttr('disabled');

        $('#si_oro_naso_swab_date_received_in_ritm_hour').removeAttr('disabled');
        $('#si_oro_naso_swab_date_received_in_ritm_minutes').removeAttr('disabled');

    // END

    //3RD ROW
        $('#si_other_if_yes_date_collected').removeAttr('disabled');
        $('#si_other_date_sent_to_ritm').removeAttr('disabled');
        $('#si_other_date_received_in_ritm').removeAttr('disabled');
        $('#si_other_virus_isolation_result').removeAttr('disabled');
        $('#si_other_pcr_result').removeAttr('disabled');

        // TIME
        $('#si_other_if_yes_date_collected_hour').removeAttr('disabled');
        $('#si_other_if_yes_date_collected_minutes').removeAttr('disabled');

        $('#si_other_date_sent_to_ritm_hour').removeAttr('disabled');
        $('#si_other_date_sent_to_ritm_minutes').removeAttr('disabled');

        $('#si_other_date_received_in_ritm_hour').removeAttr('disabled');
        $('#si_other_date_received_in_ritm_minutes').removeAttr('disabled');
        
    // END

    //Other
    remove_disable_current();
    $('#ph_residence_region').removeAttr('disabled');
    $('#ph_residence_city_municipality').removeAttr('disabled');

    pregnant_fields_remove_disabled();
    no_xray_done_remove_disabled();
}

function isNumberKey_with_dash(evt){
    
    var charCode = (evt.which) ? evt.which : event.keyCode
    
    if (charCode > 31 && (charCode < 48 || charCode > 57)){
        if (charCode == 45) {
            return true;
        }else{
            return false;
        }
    }else{
        return true;    
    }            

}
  
function checkFields_update(){

    // Investigation
    var laboratory_number = $('#laboratory_number').val().trim();
    var disease_reporting_unit_hospital = $('#disease_reporting_unit_hospital').val().trim();
    var name_of_investigator = $('#name_of_investigator').val().trim();
    var date_of_interview = $('#date_of_interview').val().trim();

    // Patient Profile
    var patient_lastname = $('#patient_lastname').val().trim();
    var patient_firstname = $('#patient_firstname').val().trim();
    var patient_middle_name = $('#patient_middle_name').val().trim();
    var patient_birthday = $('#patient_birthday').val().trim();
    var patient_age = $('#patient_age').val().trim();
    var patient_sex = $('#patient_sex').val().trim();
    var patient_occupation = $('#patient_occupation').val().trim();
    var patient_civil_status = $('#patient_civil_status').val().trim();
    var patient_nationality = $('#patient_nationality').val().trim();

    var ofw_employers_name = $('#ofw_employers_name').val();
    
    // PH Residence
    var ph_residence_city_municipality = $('#ph_residence_city_municipality').val().trim();
    var ph_residence_province = $('#ph_residence_province').val().trim();
    var ph_residence_region = $('#ph_residence_region').val().trim();

    var patient_cellphone_number = $('#ph_residence_cellphone_no').val().trim();
    // Exposure History
    // var exposure_history = check_eh();

    // Specimen Information
    var specimen_information = check_sp();

    if(specimen_information == false || ofw_employers_name == '' || patient_cellphone_number == '' || ph_residence_city_municipality == '' || ph_residence_province == '' ||  ph_residence_region == '' || patient_middle_name == '' || laboratory_number == '' || patient_lastname == '' || patient_firstname == '' || patient_birthday  == '' || patient_age  == '' || patient_sex  == '' || disease_reporting_unit_hospital == '' || date_of_interview == ''){
        $('#cif-alert-create-success').css('display','none');
        $('#cif-alert-empty-fields').css('display','block');
    }else{  
        var rewq = $('#rewq').val();
        var qwerwww = $('#qwerwww').val();
            if (rewq == "false" && qwerwww == "false") {
              $('#cif-alert-empty-fields').css('display','none'); 

              $('#cifUpdateModal').modal('toggle');
            }
    }
}

function check_eh(){
    if ($('input[name=eh_exposure_to_known_covid_case]:checked').length > 0) {
        return true;
    }else{
        return false;
    }
}

function check_sp(){
	// get status
  	var si_serum = document.getElementById("si_serum");
  	var si_oro_naso_swab = document.getElementById("si_oro_naso_swab");
	  var si_oro_swab = document.getElementById("si_oro_swab");
	  var si_naso_swab = document.getElementById("si_naso_swab");
  	var si_other = document.getElementById("si_other");

  	// Get corresponding fields
  	var si_serum_if_yes_date_collected = $('#si_serum_if_yes_date_collected').val();
    var si_serum_date_sent_to_ritm = $('#si_serum_date_sent_to_ritm').val();
    var si_serum_date_received_in_ritm = $('#si_serum_date_received_in_ritm').val();

    var si_oro_naso_swab_if_yes_date_collected = $('#si_oro_naso_swab_if_yes_date_collected').val();
	var si_oro_naso_swab_date_sent_to_ritm = $('#si_oro_naso_swab_date_sent_to_ritm').val();
	var si_oro_naso_swab_date_received_in_ritm = $('#si_oro_naso_swab_date_received_in_ritm').val();

	var si_oro_swab_if_yes_date_collected = $('#si_oro_swab_if_yes_date_collected').val();
  var si_oro_swab_date_received_in_ritm = $('#si_oro_swab_date_received_in_ritm').val();

	var si_naso_swab_if_yes_date_collected = $('#si_naso_swab_if_yes_date_collected').val();
  var si_naso_swab_date_received_in_ritm = $('#si_naso_swab_date_received_in_ritm').val();
  

  	var si_other_if_yes_date_collected = $('#si_other_if_yes_date_collected').val();
	var si_other_date_sent_to_ritm = $('#si_other_date_sent_to_ritm').val();
	var si_other_date_received_in_ritm = $('#si_other_date_received_in_ritm').val();
  
    // console.log(si_other_if_yes_date_collected);
  	// If the checkbox is checked, display the output text
  	if (si_serum.checked == true){
  		if (si_serum_if_yes_date_collected.trim() == '' || si_serum_date_received_in_ritm.trim() == '') {
  			return false;
  		}else{
  			return true;
  		}
  	}else if(si_oro_naso_swab.checked == true){
  		if (si_oro_naso_swab_if_yes_date_collected.trim() == '' || si_oro_naso_swab_date_received_in_ritm.trim() == '') {
  			return false;
  		}else{
  			return true;
  		}
  	}else if(si_oro_swab.checked == true){
  		if (si_oro_swab_if_yes_date_collected.trim() == '' || si_oro_swab_date_received_in_ritm.trim() == '') {
  			return false;
  		}else{
  			return true;
  		}
  	}else if(si_naso_swab.checked == true){
  		if (si_naso_swab_if_yes_date_collected.trim() == '' || si_naso_swab_date_received_in_ritm.trim() == '') {
  			return false;
  		}else{
  			return true;
  		}
  	}else if(si_other.checked == true){
  		if (si_other_if_yes_date_collected.trim() == '' || si_other_date_received_in_ritm.trim() == '') {
  			return false;
  		}else{
  			return true;
  		}
  	}else{
  		return false;
  	}
} 


function checkFields_update_with_remarks(){

    var laboratory_number = $('#laboratory_number').val();
    var patient_lastname = $('#patient_lastname').val();
    var patient_firstname = $('#patient_firstname').val();
    var patient_birthday = $('#patient_birthday').val();
    var patient_age = $('#patient_age').val();
    var patient_sex = $('#patient_sex').val();
    var patient_civil_status = $('#patient_civil_status').val();
    var disease_reporting_unit_hospital = $('#disease_reporting_unit_hospital').val();
    var name_of_investigator = $('#name_of_investigator').val();
    var date_of_interview = $('#date_of_interview').val();
    var patient_middle_name = $('#patient_middle_name').val();

    var ofw_employers_name = $('#ofw_employers_name').val();

    // PH Residence
    var ph_residence_city_municipality = $('#ph_residence_city_municipality').val().trim();
    var ph_residence_province = $('#ph_residence_province').val().trim();
    var ph_residence_region = $('#ph_residence_region').val().trim();

    // Specimen Information
    var specimen_information = check_sp();

  if(specimen_information == false || ofw_employers_name == '' || patient_middle_name == '' || laboratory_number == '' || patient_lastname == '' || patient_firstname == '' || patient_birthday  == '' || patient_age  == '' || patient_sex  == '' ||  disease_reporting_unit_hospital == '' || date_of_interview == '' || ph_residence_city_municipality == '' || ph_residence_province == '' ||  ph_residence_region == ''){
    $('#cif-alert-create-success').css('display','none');
    $('#cif-alert-empty-fields').css('display','block');
  }else{


    var rewq = $('#rewq').val();
        var qwerwww = $('#qwerwww').val();
            if (rewq == "false" && qwerwww == "false") {
              $('#cif-alert-empty-fields').css('display','none'); 
              $('#ValidateModalWithRemarks').modal('toggle');
            }
    
  }
}

function mytable(){

     $('#dttable').dataTable( {
      "oLanguage": {
         "sSearch": "Filter by Municipality",
         "sEmptyTable": "No Records Found.",
         "sZeroRecords": "No Matching Records."
       },
      "order": false,
      aLengthMenu: [
          [10,15,25],
          [10,15,25]
      ]

  } );
}

$("#barangay_inpt").on('keyup', function (e) {
    if (e.key === 'Enter' || e.keyCode === 13) {
      var string = $("#barangay_inpt").val();
        if(string.length >= 2){
         loadBarangay();
      }
    }
});

function openBrgy(){
  $('#address_type').val('perma');
  $('#barangaymodal').modal('toggle');
}
function openBrgy_current(){
  $('#address_type').val('curr');
  $('#barangaymodal').modal('toggle');  
}

function loadBarangay(){
    var token = $("#barangay_inpt").val();
        if(token.length >= 2){
            $.ajax({
                url   : "https://cdcdc.dlshsi.edu.ph/CIF/tqdsfasdqefsf",
                method : "POST",
                data : {data_action: 'eqwssdq',token: token},
                dataType: "json",
                success : function(data){
                    var array = data.data;

                    if(data.msg == "issda" && array.length > 0){
                      $('#nmf-barangay').css('display','none');
                        $('#dttable_wrapper').remove();
                        var content = '';
                        jQuery.each( data.data, function( i, val ) {
                            content += '<tr class="fs-12 font-weight-bold" ><td>'+val['rtndata_barangayname']+'</td><td>'+val['rtndata_townname']+'</td><td class="text-center"><button class="btn btn-primary" style="font-size: 11px;" onclick="selectedBrgy('+val['rtndata_barangayid']+')">select</button></td></tr>';
                        });
                        $('#brgy_table_wrapper').append("<table id='dttable' class='table table-bordered table-hover table-striped dataTable no-footer'  style='width:100%;'><thead style='color: #ffffff;text-align: left !important;background-color: #4f8c72;' class='header-table-registered fs-13' ><tr class='header-table-register'><th>Barangay</th><th >Town / City</th><th class='text-center'>Action</th></tr></thead><tbody class='content-table-body' ></tbody></table>  ");
                        $( ".content-table-body" ).append(content);
                        mytable();
                        
                    }else{
                      $('#dttable_wrapper').remove();
                        $('#nmf-barangay').css('display','block');
                    }
               
                }
    
            });
      }
      
}

function selectedBrgy(token_id){
  console.log(token_id);
  $('#dttable_wrapper').remove();
  $("#barangay_inpt").val('');
  $('#barangaymodal').modal('hide');
  var type = $("#address_type").val();
  $.ajax({
                url   : "https://cdcdc.dlshsi.edu.ph/CIF/jkljklhasdfnmxcvvnxcm",
                method : "POST",
                data : {data_action: 'rfgrhhher',token: token_id},
                dataType: "json",
                success : function(data){
                    var array = data.data;

                    if(data.msg == "issda" && array.length > 0){
                      jQuery.each( data.data, function( i, value ) {

                        if (type == "perma") {
                          $('#ph_residence_street').val(value['rtndata_barangayname']);
                          $('#ph_residence_city_municipality').val(value['rtndata_townname']);
                          $('#ph_residence_province').val(value['rtndata_provincename']);
                          $('#ph_residence_region').val(value['rtndata_regionname']);
                        }
                        if (type == "curr") {
                          $('#ph_residence_street_current').val(value['rtndata_barangayname']);
                          $('#ph_residence_city_municipality_current').val(value['rtndata_townname']);
                          $('#ph_residence_province_current').val(value['rtndata_provincename']);
                          $('#ph_residence_region_current').val(value['rtndata_regionname']);
                        }
                        
                        });
                    }else{
                      
                    }
               
                }
    
    });

}
// FACILITIES
function openFacility(){
    $("#facility_inpt").focus();
  $('#facilitymodal').modal('toggle');

}
function mytable_facility(){

     $('#dttable_faclity').dataTable( {
      "oLanguage": {
         "sSearch": "Search the Table",
         "sEmptyTable": "No Records Found.",
         "sZeroRecords": "No Matching Records."
       },
      "order": false,
      aLengthMenu: [
          [10,15,25],
          [10,15,25]
      ]

  } );
}

$("#facility_inpt").on('keyup', function (e) {
    if (e.key === 'Enter' || e.keyCode === 13) {
      var string = $("#facility_inpt").val();
        if(string.length >= 2){
         loadFacility();
      }
    }
});

function loadFacility(){
    var token = $("#facility_inpt").val();
    $('#dttable_faclity').remove();
        if(token.length >= 2){
            $.ajax({
                url   : "https://cdcdc.dlshsi.edu.ph/CIF/fiklxijeqfoijasdfij",
                method : "POST",
                data : {data_action: 'asdfasdf',token: token},
                dataType: "json",
                success : function(data){
                    var array = data.data;

                    if(data.msg == "issda" && array.length > 0){
                      $('#nmf-facility').css('display','none');
                        $('#dttable_faclity').remove();
                        var content = '';
                        jQuery.each( data.data, function( i, val ) {
                            content += '<tr class="fs-12 font-weight-bold" ><td>'+val['rtndata_facilityname']+'</td><td class="text-center"><button class="btn btn-primary" style="font-size: 11px;" onclick="selectedFacility(\'' + val['rtndata_facilityname'] + '\')">select</button></td></tr>';
                        });
                        $('#facility_table_wrapper').append("<table id='dttable_faclity' class='table table-bordered table-hover table-striped dataTable no-footer'  style='width:100%;'><thead style='color: #ffffff;text-align: left !important;background-color: #4f8c72;' class='header-table-registered fs-13' ><tr class='header-table-register'><th>Facility</th><th class='text-center'>Action</th></tr></thead><tbody class='content-table-body' ></tbody></table>  ");
                        $( ".content-table-body" ).append(content);
                        mytable();
                        
                    }else{
                      $('#dttable_faclity').remove();
                        $('#nmf-facility').css('display','block');
                    }
               
                }
    
            });
      }else{
        $('#dttable_faclity_wrapper').remove();
            $('#nmf-facility').css('display','block');
      }
      
}

function selectedFacility(token){
  $('#dttable_faclity_wrapper').remove();
  $("#facility_inpt").val('');
  $('#facilitymodal').modal('hide');
  $("#disease_reporting_unit_hospital").val(token);
}
