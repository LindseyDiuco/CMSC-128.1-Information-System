// TRAVEL HISTORY DATE VALIDATION
$('#th_date_of_departure').change(function() { 
    var date_departed = new Date($('#th_date_of_departure').val());
    var date_arrival = new Date($('#th_date_of_arrival_in_ph').val());

    if (date_departed > date_arrival) {
        $('#th_date_of_departure').val('');
        $('#travelhistoryDatesModal').modal('toggle');
    }
});

$('#th_date_of_arrival_in_ph').change(function() { 
    var date_departed = new Date($('#th_date_of_departure').val());
    var date_arrival = new Date($('#th_date_of_arrival_in_ph').val());

    if (date_arrival < date_departed) {
        $('#th_date_of_arrival_in_ph').val('');
        $('#travelhistoryDatesModal').modal('toggle');
    }
});
// END


// SPECIMENT DATE VALIDATION SERUM
$('#si_serum_if_yes_date_collected').change(function() { 
    var date_collected = new Date($('#si_serum_if_yes_date_collected').val());
    var date_send = new Date($('#si_serum_date_sent_to_ritm').val());
    var date_received = new Date($('#si_serum_date_received_in_ritm').val());

    if (date_collected > date_send || date_collected > date_received) {
        $('#si_serum_if_yes_date_collected').val('');
        $('#specimenDatesModal').modal('toggle');
    }
});

$('#si_serum_date_sent_to_ritm').change(function() { 
    var date_collected = new Date($('#si_serum_if_yes_date_collected').val());
    var date_send = new Date($('#si_serum_date_sent_to_ritm').val());
    var date_received = new Date($('#si_serum_date_received_in_ritm').val());

    if (date_send < date_collected || date_send > date_received) {
        $('#si_serum_date_sent_to_ritm').val('');
        $('#specimenDatesModal').modal('toggle');
    }
});

$('#si_serum_date_received_in_ritm').change(function() { 
    var date_collected = new Date($('#si_serum_if_yes_date_collected').val());
    var date_send = new Date($('#si_serum_date_sent_to_ritm').val());
    var date_received = new Date($('#si_serum_date_received_in_ritm').val());

    if (date_received < date_send || date_received < date_collected) {
        $('#si_serum_date_received_in_ritm').val('');
        $('#specimenDatesModal').modal('toggle');
    }
});

