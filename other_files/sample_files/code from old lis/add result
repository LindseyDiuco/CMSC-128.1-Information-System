  <script>
    $(document).ready(function(){

        $( "#accession_no" ).datepicker(
        {   
          maxDate: 0,
          changeMonth: true,
          changeYear: true,
          yearRange: "2022:+10",
          minDate: new Date(1920, 1 - 1, 1),
          dateFormat: 'mm-dd-yy'
        });

         var preference_set = "1";
         if (preference_set == "0") {
            $('#setup_performed_by').modal('toggle');
         }

         var p_array = ["18","28"];
         for (var key in p_array) {
            if (p_array.hasOwnProperty(key)) {
                console.log(p_array[key]);
                $('#pb_'+p_array[key]).prop("checked",true);
            }
         }
