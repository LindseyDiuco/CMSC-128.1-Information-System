      function save_preference(){
        var le = document.querySelectorAll('input[name="performed_by_setup[]"]:checked').length;
        if (le > 2 || le == 0) {
            $('#max_names').css('display','inline-block');
        }else{
            $('#max_names').css('display','none');
            $('#spf_form').submit();
        }
        
      }
  </script>

